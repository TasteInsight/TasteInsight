    "sass-loader": "^10.5.2",
    "tailwindcss": "3.4.1",
    "ts-jest": "27.1.5",
    "typescript": "^4.9.4",
    "vite": "5.0.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^1.6.0",
    "vue-tsc": "^1.0.24",
    "weapp-tailwindcss": "^4.7.8"
  },
  "pnpm": {
    "overrides": {
      "vue": "3.4.21",
      "@vue/server-renderer": "3.4.21",
      "@vue/runtime-dom": "3.4.21",
      "@vue/runtime-core": "3.4.21",
      "@vue/reactivity": "3.4.21",
      "@vue/shared": "3.4.21",
      "@dcloudio/types": "3.4.19"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "webpack"
      ],
      "allowedVersions": {
        "vue": "3.4.21",
        "@dcloudio/types": "3.4.19"
